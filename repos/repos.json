[
  {
    "kernelSource": {
      "name": "Pissarro-KernelSU",
      "repo": "https://github.com/sonrixmomo/Xiaomi_Kernel_OpenSource",
      "branch": "pissarro-r-oss",
      "device": "pissarro",
      "defconfig": "pissarro_user_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/android11-release/clang-r383902.tar.gz",
        "name": "clang",
        "binPath": ["./bin"]
      },
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9 aarch64-linux-android-4.9",
        "name": "gcc",
        "binPath": ["bin"]
      }
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_arm_arm-linux-androideabi-4.9 arm-linux-androideabi-4.9",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "gcc/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-androidkernel-",
        "CROSS_COMPILE_ARM32": "gcc/bin/arm-linux-androidkernel-",
        "LD": "clang/bin/ld.lld",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/SakuraNotStupid/AnyKernel3/",
      "branch": "master"
    }
  }
]
