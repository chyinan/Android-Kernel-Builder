[
  {
    "kernelSource": {
      "name": "Pissarro-KernelSU",
      "repo": "https://github.com/sonrixmomo/Xiaomi_Kernel_OpenSource",
      "branch": "pissarro-r-oss",
      "device": "pissarro",
      "defconfig": "pissarro_user_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/android11-release/clang-r383902b.tar.gz",
        "branch": "android11-release",
        "name": "clang",
        "binPath": ["./bin"]
      },
      {
        "url" "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/heads/android11-release.tar.gz",
        "name": "gcc",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CLANG_TRIPLE": "gcc/bin/aarch64-linux-android-",
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "gcc/bin/arm-linux-androideabi-",
        "LD": "clang/bin/ld.lld",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/SakuraNotStupid/AnyKernel3/",
      "branch": "master"
    }
  }
]
