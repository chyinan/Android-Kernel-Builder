[
  {
    "kernelSource": {
      "name": "v300l_KernelSU",
      "repo": "https://github.com/chyinan/kernel_lge_msm8998",
      "branch": "main",
      "device": "v300l",
      "defconfig": "joan_lgu_kr-perf_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://releases.linaro.org/components/toolchain/binaries/latest-4/aarch64-elf/gcc-linaro-4.9.4-2017.01-x86_64_aarch64-elf.tar.xz",
        "name": "gcc",
        "binPath": ["./bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CONFIG_USB_HOST_NOTIFY"="y"         
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "master"
    }
  }
]
