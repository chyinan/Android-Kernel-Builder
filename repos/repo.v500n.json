[
    {
      "kernelSource": {
        "name": "Dragon-V500N-KernelSU",
        "repo": "https://github.com/sonrixmomo/android_kernel_lge_sm8150",
        "branch": "main",
        "device": "V500N",
        "defconfig": "vendor/dragon_flash_defconfig"
      },
      "withKernelSU": true,
      "toolchains": [
        {
          "url": "https://github.com/llvm/llvm-project/releases/download/llvmorg-13.0.1/clang+llvm-13.0.1-aarch64-linux-gnu.tar.xz",
          "name": "clang",
          "binPath": ["bin"]
        },
        {
          "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
          "branch": "lineage-17.0",
          "name": "gcc64",
          "binPath": ["bin"]
        },

      ],
      "ccache": true,
      "params": {
        "ARCH": "arm64",
        "CC": "clang/bin/clang",
        "externalCommand": {
          "CLANG_TRIPLE": "aarch64-linux-gnu-",
          "CROSS_COMPILE": "gcc64/aarch64-linux-android-",
          "CROSS_COMPILE_ARM32": "gcc/arm-linux-androideabi-",
          "LD": "clang/bin/ld.lld",
          "AR": "clang/bin/llvm-ar",
          "NM": "clang/bin/llvm-nm",
          "OBJCOPY": "clang/bin/llvm-objcopy",
          "OBJDUMP": "clang/bin/llvm-objdump",
          "READELF": "clang/bin/llvm-readelf",
          "OBJSIZE": "clang/bin/llvm-size",
          "STRIP": "clang/bin/llvm-strip",
          "LDGOLD": "clang/bin/aarch64-linux-gnu-ld.gold",
          "LLVM_AR": "clang/bin/llvm-ar",
          "LLVM_DIS": "clang/bin/llvm-dis"
        }
      },
      "AnyKernel3": {
        "use": true,
        "release": false,
        "repo": "https://github.com/sonrixmomo/AnyKernel3",
        "branch": "master"
      }
    }
  ]
  
