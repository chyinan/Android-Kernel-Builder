[
  {
    "kernelSource": {
      "name": "Pissarro-KernelSU",
      "repo": "https://github.com/sonrixmomo/Xiaomi_Kernel_OpenSource",
      "branch": "pissarro-r-oss",
      "device": "pissarro",
      "defconfig": "pissarro_user_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
  {
    "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9/",
    "branch": "gcc",
    "name": "gcc",
    "binPath": ["bin"]
  },
  {
    "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/heads/android11-release.tar.gz",
    "name": "gcc64",
    "binPath": ["bin"]
  },
  {
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9/+archive/refs/heads/android11-release.tar.gz",
    "name": "clang",
    "binPath": ["bin"]
  }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc64/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "gcc/arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/sonrixmomo/AnyKernel3",
      "branch": "master"
    }
  }
]